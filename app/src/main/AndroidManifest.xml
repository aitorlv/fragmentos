<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aitor.inmobiliaria" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.aitor.inmobiliaria.Principal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.aitor.inmobiliaria.Insertar"
            android:label="@string/title_activity_insertar" >
        </activity>
        <activity
            android:name="com.example.aitor.inmobiliaria.Modificar"
            android:label="@string/title_activity_modificar" >
        </activity>
        <activity
            android:name="com.example.aitor.inmobiliaria.Secundaria"
            android:label="@string/title_activity_secundaria" >
        </activity>
    </application>

</manifest>
